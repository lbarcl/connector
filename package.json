{
  "name": "connector",
  "version": "1.0.0",
  "description": "A HTTP client for devs that love low-level communication",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "test": "tsc&&node ./dist/test.js",
    "jest": "jest --detectOpenHandles --forceExit",
    "build": "tsc"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lbarcl/connector.git"
  },
  "author": "lbarcl",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lbarcl/connector/issues"
  },
  "homepage": "https://github.com/lbarcl/connector#readme",
  "devDependencies": {
    "@types/jest": "^28.1.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.38",
    "jest": "^28.1.0",
    "ts-jest": "^28.0.4",
    "typescript": "^4.7.3"
  }
}
